<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
  <mapper namespace="com.qianfeng.goutrip.zcy.bean.InsMapper">
  <select id="insertIns" parameterType="com.qianfeng.goutrip.zcy.bean.InsDTO">
  	insert tb_inspiration(ins_name,ins_whither,ins_days,ins_phone,ins_remaks,ins_status,ins_user_id) 
  	values(#{ins_name},#{ins_whither},#{ins_days},#{ins_phone},#{ins_remaks},#{ins_status},#{ins_user_id})
